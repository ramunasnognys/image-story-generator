{
    "version": 2,
    "builds": [
      { 
        "src": "frontend/package.json",
        "use": "@vercel/next"
      },
      { "src": "api/**/*.py", "use": "@vercel/python" }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/api/$1" },
      { "src": "/(.*)", "dest": "/frontend/$1" }
    ]
  }